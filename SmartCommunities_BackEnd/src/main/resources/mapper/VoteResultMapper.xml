<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ltbyz.practice_demo.wisdom_block.mapper.VoteResultMapper">
    <select id="queryAll" resultType="ltbyz.practice_demo.wisdom_block.pojo.VoteResult">
        select * from VoteResult
    </select>

    <select id="queryOne" resultType="ltbyz.practice_demo.wisdom_block.pojo.VoteResult">
        select * from VoteResult where VoteID = #{VoteID}
    </select>

</mapper>